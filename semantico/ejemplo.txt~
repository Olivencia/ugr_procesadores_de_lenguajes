principal
{

	var
		entero ve, pepe[2,3], manolo;
		real v[9], pepita ,pepepe, manolita, wete;
		caracter vc ;
		logico  vl[2,1] ;
		entero pe[2,3], pe2 ;
		real pf, pf2[5,6] ;
		caracter pc[3,4], pc2 ;
		logico  pl[3,3] ;
	finvar

	entero[2,3] funcionA (entero a1, logico a12[4,5], entero a2[4,3], real a2, caracter a3)
	{
		var
			entero x1, x2 ;
		finvar

		caracter funcionB (caracter b1, logico b2)
		{
			var
				real xf[2,3], x2 ;
			finvar

			real funcionC (logico c1, entero c2)
			{
				var 
					real x1 ;
				finvar
				x1= 1.3*2+23/verdadero ;
				si(c2>10+42*32/a43 && x3/b){
					c2= c2/1+2 ;
				}
				sino{
					x1= 3.1;
				}
				devolver x1 ;
			}

			xf= functionC (verdadero, 10);
			x2= xf*(funcionC(falso,1)-funcionC(verdadero,23))/10.0+2;
			mientras(x2/funcionC(falso,1)-xf<10.0) {
				x2= x2/xf ;
			}
		}

		real funcionD (real d1)
		{
			var
				caracter dato ;
				entero valor ;
			finvar
			caracter funcionE (caracter e1,caracter e2)
			{
				escribir "introduzca_dos caracteres:";
				leer e1, e2 ;
				si(e1=='a'){
					p3 = 3/1;
					devolver e1 ;
				}
				sino {
					si(e1=='b'){
						devolver e2 ;
					}
					sino{
						devolver '_';
					}
				}
			}

			escribir "introduzca_un_valor_entero:";
			leer valor ;
			si(d1>0.0)
			{
				var
					entero dato ;
				finvar
				dato= 2 ;
				dato= valor*20/dato ;
			}
			sino
			{
				valor= valor*100 ;
				d1= d1/1000.0 ;
			}
			devolver d1 ;
		}

		pe= pe-10 ;
		pf= pf-10.0 ;
		pc= pc-'#' ;

		si(?(pe<20) == 20) {
			ve= pe-3 ;
		}
		sino
		{
			pe= pe*pe2 ;
			pe= pe2 - pe - 10*(20/2000) ;
		}
	}
	devolver 3;
}
